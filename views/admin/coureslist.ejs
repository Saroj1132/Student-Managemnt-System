<%- include('.../../../paritals/header') %>
<div class="containers">
  <a href="/Course/Addcourse" class="btn btn-primary">Add New Course Page</a>

  <br>
  <br>
            <div class="row">
                <h1>Teacher Page List</h1>
                <table class="table table-striped table-primary">
                    <thead>
                        <tr>
                            <th>Courser_Name</th>
                            <th>Action</th>
                        </tr>
                    </thead>
                    <tbody>
                        <%row.forEach(function(p){ %>
                        <tr>
                            <td><%=p.Courser_Name%></td>
                            <td>
                            <a class="btn btn-primary btn-xs" href="/Course/edit/<%=p._id%>"><i class="glyphicon glyphicon-edit"></i></a>
                            <a class="btn btn-danger btn-xs" href="/Course/delete/<%=p._id%>"><i class="glyphicon glyphicon-trash"></i></a>
                            </td>
                        </tr>
                        <% }) %>
                    </tbody>
                </table>
            </div>
        </div>
