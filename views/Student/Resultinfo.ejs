<%- include('../paritals/studentheader') %>
<div class="container">

  <br>
            <div class="row">
                <h1>ResultInfo</h1>
                <table class="table table-striped table-primary">
                    <thead>
                        <tr>
                            <th>Exam_Name</th>
                            <th>Student_Name</th>
                            <th>Subject</th>
                            <th>Marks</th>
                        </tr>
                    </thead>
                    <tbody>
                        <%row.forEach(function(p){ %>
                        <tr>
                            <td><%=p.Exam_Id.Name%></td>
                            <td><%=p.Student_id.Fname%> <%=p.Student_id.Lname%></td>
                            <td><%=p.Course_id.Courser_Name%></td>
                            <td><%=p.Marks%></td>

                            
                        </tr>
                        <% }) %>
                    </tbody>
                </table>
            </div>
        </div>
